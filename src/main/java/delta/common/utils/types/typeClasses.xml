<?xml version="1.0" encoding="UTF-8"?>
<TYPE_CLASSES>
  <TYPE_CLASS NAME="BOOLEAN" JAVACLASS="delta.common.utils.types.BooleanType">
  </TYPE_CLASS>
  <TYPE_CLASS NAME="INTEGER" JAVACLASS="delta.common.utils.types.IntegerType">
    <PARAM NAME="MIN" TYPE="INTEGER" DEFAULT="0" MANDATORY="true" />
    <PARAM NAME="MAX" TYPE="INTEGER" DEFAULT="INFINITY" MANDATORY="true" />
  </TYPE_CLASS>
  <TYPE_CLASS NAME="STRING" JAVACLASS="delta.common.utils.types.StringType">
    <PARAM NAME="MIN_LENGTH" TYPE="POSITIVE_INTEGER" DEFAULT="0" MANDATORY="true" />
    <PARAM NAME="MAX_LENGTH" TYPE="POSITIVE_INTEGER" DEFAULT="INFINITY" MANDATORY="true" />
    <PARAM NAME="MULTILINES" TYPE="BOOLEAN" DEFAULT="false" MANDATORY="true" />
    <PARAM NAME="REGEXP" TYPE="STRING" MANDATORY="false" />
  </TYPE_CLASS>
  <TYPE_CLASS NAME="CHOICE" JAVACLASS="delta.common.utils.types.ChoiceType">
    <PARAM NAME="MULTIPLE" TYPE="BOOLEAN" DEFAULT="false" MANDATORY="true" />
    <PARAM NAME="TYPE" TYPE="TYPE" DEFAULT="STRING" MANDATORY="true" />
    <PARAM NAME="OPTION" TYPE="STRING" MANDATORY="false" />
  </TYPE_CLASS>
  <TYPE_CLASS NAME="TYPE" JAVACLASS="delta.common.utils.types.TypeType">
    <PARAM NAME="NAMESPACE" TYPE="STRING" DEFAULT="DEFAULT" MANDATORY="true" />
  </TYPE_CLASS>
</TYPE_CLASSES>
